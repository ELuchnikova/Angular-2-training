{
  "name": "weathr-app",
  "version": "1.0.0",
  "description": "A webpack starter for Angular",
  "scripts": {
    "start": "webpack-dev-server --hot --progress",
    "test": "karma start karma.config.js",
    "dev-serve": "http-server ./ -a 0.0.0.0 -p 9002",
    "prod-serve": "http-server ./ -a 0.0.0.0 -p 9000",
    "ngc": "ngc -p tsconfig-aot.json",
    "dev": "set NODE_ENV=development&&webpack --config webpack.config.js && npm run dev-serve",
    "build": "npm run ngc && set NODE_ENV=production&&webpack -p --config prod.webpack.config.js && npm run prod-serve"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "~2.4.0",
    "@angular/compiler": "~2.4.0",
    "@angular/compiler-cli": "^2.4.1",
    "@angular/core": "~2.4.0",
    "@angular/forms": "~2.4.0",
    "@angular/http": "~2.4.0",
    "@angular/material": "^2.0.0-beta.1",
    "@angular/platform-browser": "~2.4.0",
    "@angular/platform-browser-dynamic": "~2.4.0",
    "@angular/router": "~3.4.0",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^2.0.0",
    "@ngrx/store": "^2.2.1",
    "@ngrx/store-devtools": "^3.2.3",
    "core-js": "^2.4.1",
    "jasmine-core": "^2.5.2",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.1",
    "zone.js": "^0.7.4"
  },
  "devDependencies": {
    "@angular/compiler": "^2.2.4",
    "@angular/compiler-cli": "^2.4.4",
    "@angular/platform-server": "^2.4.4",
    "@types/core-js": "^0.9.35",
    "@types/googlemaps": "^3.25.36",
    "@types/jasmine": "^2.2.34",
    "@types/node": "^6.0.45",
    "@types/whatwg-streams": "0.0.1",
    "angular2-router-loader": "^0.3.5",
    "angular2-template-loader": "^0.4.0",
    "awesome-typescript-loader": "=3.0.0-beta.10",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "codelyzer": "^2.0.0-beta.1",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "=2.0.0-beta.4",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.26.0",
    "inline-environment-variables-webpack-plugin": "^1.1.0",
    "istanbul-instrumenter-loader": "2.0.0",
    "karma": "^1.5.0",
    "karma-coverage": "^1.1.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-remap-coverage": "^0.1.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.2",
    "null-loader": "^0.1.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.5",
    "ts-loader": "=1.3.3",
    "tslint": "=4.0.2",
